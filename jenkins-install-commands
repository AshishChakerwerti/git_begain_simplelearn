1  sudo apt update && upgrade
    2  apt install openjdk-8-jdk
    3  wget -q -O - https://pkg.jenkins.io/debian-stable/jenkins.io.key | sudo apt-key add  
    4  sh -c 'echo deb http://pkg.jenkins.io/debian-stable binary/ > /etc/apt/sources.list.d/jenkins.list'
    5  apt update 
    6  apt install jenkins 
    7  systemctl jenkins status
    8  systemctl status jemkins
    9  systemctl start jenkins
   10  systemctl status jenkins
